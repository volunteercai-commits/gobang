{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "五子棋",
  "version": "3.0.0",
  "identifier": "com.volunteercai.gobang",
  "build": {
    "beforeDevCommand": "npm run start",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../build"
  },
        "app": {
          "windows": [
            {
              "fullscreen": false,
              "resizable": true,
              "title": "五子棋",
              "width": 400,
              "height": 600,
              "minWidth": 350,
              "minHeight": 500,
              "maxWidth": 600,
              "maxHeight": 800
            }
          ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/256x256.png",
      "icons/512x512.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
